{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint ./ --ext ts,tsx",
    "prepare": "cd .. && husky install client/.husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
